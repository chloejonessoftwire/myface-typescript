<html lang="en-gb">
<%- include('_head') -%>
<%- include('_header') -%>
<body>
<h1>Users</h1>
<ul>
    <% results.forEach(function(user) {%>
        <li>
            <%= user.name %>, username: <%= user.username %>, email: <%= user.email %>
            <br>
            <img src= '<%= user.profileImageUrl %>'>
            <br>
            <input type="button" onclick="window.location.href='<%= user.coverImageUrl %>';" value = "Cover Image">
        </li>
    <% }); %>
    </ul>
<%if (next) { %>
    <a href="<%= next %>">Next</a>
<% } %>
<%if (previous) { %>
    <a href="<%= previous %>">Previous</a>
<% } %>
</body>
<%- include('_footer') -%>
</html>